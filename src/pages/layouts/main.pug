include /mixins/meta

doctype html

html
  head
    block meta
      +meta()

    block stylesheets
      link(rel='stylesheet', href=assetPath('/assets/main.css'))
  
    block favicon
      link(rel="icon", href="/favicon.png" )
  
  body.orb
    block header
      include /partials/header

    main
      block content

    block footer
      include /partials/footer

    block javascripts
      script(src=assetPath('/assets/main.js'))
